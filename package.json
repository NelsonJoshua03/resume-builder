{
  "name": "careercraft-website",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "homepage": "https://careercraft.in",
  "scripts": {
    "dev": "vite --host --port 3001",
    "dev:open": "vite --host --port 3001 --open",
    "dev:firebase": "vite --host --port 3001 --cors",
    "start": "vite --host --port 3001",
    "build": "vite build",
    "build:production": "vite build --mode production",
    "build:netlify": "vite build --mode production",
    "build:analyze": "ANALYZE=true vite build --mode production",
    "preview": "vite preview --host --port 3001",
    "preview:production": "vite preview --host --port 3002",
    "serve": "vite preview --host --port 3001",
    "dev:server": "cd server && npm run dev",
    "dev:both": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "new-blog": "node add-blog-post.js",
    "blog-list": "node add-blog-post.js list",
    "blog-delete": "node add-blog-post.js delete",
    "blog-help": "node add-blog-post.js help",
    "test:firebase": "node -e \"console.log('Testing Firebase config...')\"",
    "test:analytics": "node -e \"console.log('Analytics Measurement ID:', process.env.VITE_GA_MEASUREMENT_ID || 'G-WSKZJDJW77')\"",
    "audit": "npm run build:production && npx lighthouse http://localhost:3002 --output html --output-path ./dist/lighthouse-report.html",
    "deploy": "npm run build:production && firebase deploy",
    "deploy:hosting": "npm run build:production && firebase deploy --only hosting",
    "deploy:functions": "firebase deploy --only functions",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules package-lock.json",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "security": "npm audit && npx audit-ci --moderate",
    "postinstall": "echo 'Dependencies installed successfully!'"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.0",
    "@radix-ui/react-select": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.0",
    "@radix-ui/react-tooltip": "^1.1.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.19",
    "axios": "^1.12.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "firebase": "^10.14.1",
    "gray-matter": "^4.0.3",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "lucide-react": "^0.555.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.28.0",
    "react-share": "^5.2.2",
    "react-to-print": "^3.1.1",
    "recharts": "^2.15.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "rss-parser": "^3.13.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/html2canvas": "^0.5.35",
    "@types/jspdf": "^1.3.3",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-legacy": "^5.4.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.21",
    "concurrently": "^7.6.0",
    "cssnano": "^6.1.2",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^5.4.21",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-pwa": "^0.20.2"
  },
  "description": "CareerCraft.in - India's premier career platform for resume building and job search with ATS optimization",
  "main": "eslint.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NelsonJoshua03/careercraft.git"
  },
  "keywords": [
    "resume-builder",
    "job-portal",
    "career-platform",
    "india-jobs",
    "ats-resume",
    "firebase",
    "react",
    "typescript",
    "vite",
    "tailwindcss"
  ],
  "author": "CareerCraft India <contact@careercraft.in>",
