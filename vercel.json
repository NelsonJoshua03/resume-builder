{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/builder",
      "destination": "/free-resume-builder",
      "permanent": true
    },
    {
      "source": "/builder/",
      "destination": "/free-resume-builder",
      "permanent": true
    },
    {
      "source": "/resume-builder",
      "destination": "/free-resume-builder",
      "permanent": true
    },
    {
      "source": "/resume-builder/",
      "destination": "/free-resume-builder",
      "permanent": true
    },
    {
      "source": "/cv-builder",
      "destination": "/cv-maker-online",
      "permanent": true
    },
    {
      "source": "/cv-builder/",
      "destination": "/cv-maker-online",
      "permanent": true
    },
    {
      "source": "/resume-creator",
      "destination": "/online-resume-maker",
      "permanent": true
    },
    {
      "source": "/resume-creator/",
      "destination": "/online-resume-maker",
      "permanent": true
    },
    {
      "source": "/make-resume",
      "destination": "/create-resume-online",
      "permanent": true
    },
    {
      "source": "/make-resume/",
      "destination": "/create-resume-online",
      "permanent": true
    },
    {
      "source": "/create-resume",
      "destination": "/create-resume-online",
      "permanent": true
    },
    {
      "source": "/create-resume/",
      "destination": "/create-resume-online",
      "permanent": true
    },
    {
      "source": "/resume-maker",
      "destination": "/online-resume-maker",
      "permanent": true
    },
    {
      "source": "/resume-maker/",
      "destination": "/online-resume-maker",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "X-Robots-Tag",
          "value": "index, follow, max-image-preview:large"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains; preload"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=(), interest-cohort=()"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/logos/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(sitemap.xml|robots.txt|manifest.json)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        },
        {
          "key": "Content-Type",
          "value": "text/plain; charset=utf-8"
        }
      ]
    },
    
    {
      "source": "/(free-resume-builder|ats-resume-builder|online-resume-maker|cv-maker-online|professional-resume-builder|resume-builder-for-freshers|resume-builder-for-engineers|resume-builder-for-it-jobs|resume-builder-for-developers|create-resume-online)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, stale-while-revalidate=86400"
        },
        {
          "key": "X-Robots-Tag",
          "value": "index, follow, max-image-preview:large"
        },
        {
          "key": "Vary",
          "value": "Accept-Encoding"
        }
      ]
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true
}