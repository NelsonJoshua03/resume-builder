{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/job-drives",
      "dest": "/latest-walk-in-drives-india",
      "status": 301
    },
    {
      "src": "/job-drives/(.*)",
      "dest": "/latest-walk-in-drives-india",
      "status": 301
    },
    {
      "src": "/job-applications",
      "dest": "/latest-job-updates-india",
      "status": 301
    },
    {
      "src": "/job-applications/(.*)",
      "dest": "/latest-job-updates-india",
      "status": 301
    },
    {
      "src": "/latest-jobs-for-freshers-india",
      "dest": "/latest-job-updates-india",
      "status": 301
    },
    {
      "src": "/latest-jobs-for-freshers-india/(.*)",
      "dest": "/latest-job-updates-india",
      "status": 301
    },
    {
      "src": "/builder",
      "dest": "/free-resume-builder",
      "status": 301
    },
    {
      "src": "/builder/",
      "dest": "/free-resume-builder",
      "status": 301
    },
    {
      "src": "/resume-builder",
      "dest": "/free-resume-builder",
      "status": 301
    },
    {
      "src": "/resume-builder/",
      "dest": "/free-resume-builder",
      "status": 301
    },
    {
      "src": "/cv-builder",
      "dest": "/cv-maker-online",
      "status": 301
    },
    {
      "src": "/cv-builder/",
      "dest": "/cv-maker-online",
      "status": 301
    },
    {
      "src": "/resume-creator",
      "dest": "/online-resume-maker",
      "status": 301
    },
    {
      "src": "/resume-creator/",
      "dest": "/online-resume-maker",
      "status": 301
    },
    {
      "src": "/make-resume",
      "dest": "/create-resume-online",
      "status": 301
    },
    {
      "src": "/make-resume/",
      "dest": "/create-resume-online",
      "status": 301
    },
    {
      "src": "/create-resume",
      "dest": "/create-resume-online",
      "status": 301
    },
    {
      "src": "/create-resume/",
      "dest": "/create-resume-online",
      "status": 301
    },
    {
      "src": "/resume-maker",
      "dest": "/online-resume-maker",
      "status": 301
    },
    {
      "src": "/resume-maker/",
      "dest": "/online-resume-maker",
      "status": 301
    },
    {
      "src": "/ats-resume",
      "dest": "/ats-resume-builder",
      "status": 301
    },
    {
      "src": "/ats-resume/",
      "dest": "/ats-resume-builder",
      "status": 301
    },
    {
      "src": "/online-resume",
      "dest": "/online-resume-maker",
      "status": 301
    },
    {
      "src": "/online-resume/",
      "dest": "/online-resume-maker",
      "status": 301
    },
    {
      "src": "/professional-resume",
      "dest": "/professional-resume-builder",
      "status": 301
    },
    {
      "src": "/professional-resume/",
      "dest": "/professional-resume-builder",
      "status": 301
    },
    {
      "src": "/fresher-resume",
      "dest": "/resume-builder-for-freshers",
      "status": 301
    },
    {
      "src": "/fresher-resume/",
      "dest": "/resume-builder-for-freshers",
      "status": 301
    },
    {
      "src": "/engineer-resume",
      "dest": "/resume-builder-for-engineers",
      "status": 301
    },
    {
      "src": "/engineer-resume/",
      "dest": "/resume-builder-for-engineers",
      "status": 301
    },
    {
      "src": "/it-resume",
      "dest": "/resume-builder-for-it-jobs",
      "status": 301
    },
    {
      "src": "/it-resume/",
      "dest": "/resume-builder-for-it-jobs",
      "status": 301
    },
    {
      "src": "/developer-resume",
      "dest": "/resume-builder-for-developers",
      "status": 301
    },
    {
      "src": "/developer-resume/",
      "dest": "/resume-builder-for-developers",
      "status": 301
    },
    {
      "src": "/free-cv-maker",
      "dest": "/cv-maker-online",
      "status": 301
    },
    {
      "src": "/free-cv-maker/",
      "dest": "/cv-maker-online",
      "status": 301
    },
    {
      "src": "/cv-creator",
      "dest": "/cv-maker-online",
      "status": 301
    },
    {
      "src": "/cv-creator/",
      "dest": "/cv-maker-online",
      "status": 301
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/images/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.(ico|svg|png|jpg|jpeg|gif))",
      "headers": {
        "cache-control": "public, max-age=86400"
      }
    },
    {
      "src": "/(sitemap\\.xml|robots\\.txt|manifest\\.json)",
      "headers": {
        "cache-control": "public, max-age=3600",
        "content-type": "text/plain; charset=utf-8"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate",
        "x-robots-tag": "index, follow, max-image-preview:large",
        "strict-transport-security": "max-age=31536000; includeSubDomains; preload",
        "x-frame-options": "SAMEORIGIN",
        "x-content-type-options": "nosniff",
        "referrer-policy": "strict-origin-when-cross-origin",
        "permissions-policy": "camera=(), microphone=(), geolocation=(), interest-cohort=()"
      }
    }
  ]
}